{
  "name": "cap-collectif-graphql-doctor",
  "version": "1.0.0",
  "description": "Prevent GraphQL API issues",
  "author": "Aur√©lien David <aurelien@cap-collectif.com>",
  "license": "ISC",
  "repository": "https://github.com/cap-collectif/graphql-doctor.git",
  "homepage": "https://github.com/cap-collectif/graphql-doctor",
  "bugs": "https://github.com/cap-collectif/graphql-doctor/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "dev": "nodemon",
    "start": "probot run ./index.js",
    "lint": "standard --fix",
    "test": "jest && standard",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage"
  },
  "graphql-doctor": {
    "example/schema.graphql": {
      "ref": "heads/master",
      "schemaPath": "example/schema.graphql"
    }
  },
  "dependencies": {
    "extract-values": "^0.1.2",
    "graphql": "^14.0.2",
    "probot": "^7.2.0"
  },
  "devDependencies": {
    "jest": "^22.4.3",
    "nodemon": "^1.17.2",
    "smee-client": "^1.0.2",
    "standard": "^10.0.3"
  },
  "engines": {
    "node": ">= 8.3.0"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "jest": {
    "testEnvironment": "node"
  },
  "nodemonConfig": {
    "exec": "npm start",
    "watch": [
      ".env",
      "."
    ]
  }
}
